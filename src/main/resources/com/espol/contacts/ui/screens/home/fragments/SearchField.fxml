<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="4.0" type="HBox" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="textField" onAction="#search" promptText="Buscar Contacto..." HBox.hgrow="ALWAYS" />
      <Button mnemonicParsing="false" onAction="#search" styleClass="text-icon-button">
         <graphic>
            <FontIcon iconLiteral="fas-search" />
         </graphic>
      </Button>
   </children>
</fx:root>
