<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root alignment="CENTER" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="8.0" styleClass="contact-cell" type="HBox" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
    </padding>
    <Label fx:id="nameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" HBox.hgrow="ALWAYS">
        <tooltip>
          <Tooltip fx:id="nameTooltip" text="Empty Tooltip" />
        </tooltip>
    </Label>
    <MenuButton fx:id="trailing" maxHeight="1.7976931348623157E308" mnemonicParsing="false" styleClass="text-icon-button">
      <items>
          <MenuItem fx:id="favoriteMenuItem" mnemonicParsing="false" onAction="#toggleFavorite" text="Favoritos" />
          <MenuItem mnemonicParsing="false" onAction="#onEdit" text="Editar">
              <graphic>
                  <FontIcon iconLiteral="fas-edit" />
              </graphic>
          </MenuItem>
          <MenuItem mnemonicParsing="false" onAction="#onDelete" text="Eliminar">
              <graphic>
                  <FontIcon iconLiteral="fas-trash" />
              </graphic>
          </MenuItem>
      </items>
        <graphic>
          <FontIcon iconLiteral="mdmz-more_vert" iconSize="20" />
        </graphic>
    </MenuButton>
</fx:root>
